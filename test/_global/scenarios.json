  "python-dev": {
    "image": "mcr.microsoft.com/devcontainers/python:1-3-bookworm",
    "features": {
      "common-dev": {}
    }
  },
  "dotfiles": {
    "image": "mcr.microsoft.com/devcontainers/base:1-debian",
    "features": {
      "common-dev": {}
    }
  }
