{
  "common-dev": {
    "image": "mcr.microsoft.com/devcontainers/base:1-debian",
    "features": {
      "ghcr.io/devcontainers/features/common-utils": {
        "installZsh": true,
        "configureZshAsDefaultShell": true,
        "installOhMyZsh": false,
        "installOhMyZshConfig": false,
        "upgradePackages": true,
        "username": "vscode",
